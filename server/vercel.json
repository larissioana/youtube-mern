{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/users", "dest": "/api/users.js" },
      { "src": "/api/auth", "dest": "/api/auth.js" },
      { "src": "/api/videos", "dest": "/api/videos.js" },
      { "src": "/api/comments", "dest": "/api/comments.js" }
    ]
  }
  